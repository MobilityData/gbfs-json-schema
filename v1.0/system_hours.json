{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "system_hours_status.json",
  "description": "",
  "type": "object",
  "properties": {
    "last_updated": {
      "type": "integer",
      "minimum": 0,
      "maximum": 1924988399
    },
    "ttl": {
      "type": "integer",
      "minimum": 0
    },
    "data": {
      "type": "object",
      "properties": {
        "rental_hours": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "user_type": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": ["member", "nonmember"]
                }
              },
              "days": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": ["mon", "tue", "wed", "thu", "fri", "sat", "sun"]
                }
              },
              "start_time": {
                "type": "string",
                "pattern": "^[0-9]{2}:[0-9]{2}:[0-9]{2}$"
              },
              "end_time": {
                "type": "string",
                "pattern": "^[0-9]{2}:[0-9]{2}:[0-9]{2}$"
              }
            },
            "required": ["user_types", "days", "start_time", "end_time"]
          }
        }
      },
      "required": ["rental_hours"]
    }
  },
  "required": ["last_updated", "ttl", "data"]
}
