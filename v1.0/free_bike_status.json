{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "free_bikes_status.json",
  "description": "",
  "type": "object",
  "properties": {
    "last_updated": {
      "type": "integer",
      "minimum": 0,
      "maximum": 1924988399
    },
    "ttl": {
      "type": "integer",
      "minimum": 0
    },
    "data": {
      "type": "object",
      "properties": {
        "bikes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "bike_id": {
                "type": "string"
              },
              "lat": {
                "type": "number",
                "minimum": -90,
                "maximum": 90
              },
              "lon": {
                "type": "number",
                "minimum": -180,
                "maximum": 180
              },
              "is_reserved": {
                "oneOf": [{ "type": "boolean" }, { "type": "number" }]
              },
              "is_disabled": {
                "oneOf": [{ "type": "boolean" }, { "type": "number" }]
              }
            },
            "required": ["bike_id", "lat", "lon", "is_reserved", "is_disabled"]
          }
        }
      },
      "required": ["bikes"]
    }
  },
  "required": ["last_updated", "ttl", "data"]
}
