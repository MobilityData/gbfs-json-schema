{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "system_calendar.json",
  "description": "",
  "type": "object",
  "properties": {
    "last_updated": {
      "type": "integer",
      "minimum": 0,
      "maximum": 1924988399
    },
    "ttl": {
      "type": "integer",
      "minimum": 0
    },
    "data": {
      "type": "object",
      "properties": {
